{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/GetTimeServer/bin/Debug/net8.0/GetTimeServer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/GetTimeServer",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}